alias ipd="/usr/bin/get_iplayer \
  --atomicparsley=/usr/bin/atomicparsley \
  --fileprefix=\"<nameshort> - S<00seriesnum>E<00episodenum>\" \
  --output=\"/.pool/local/media/tv/\" \
  --subdir \
  --subdir-format=\"<nameshort>/Season <00seriesnum>\" \
  --tv-quality=web \
  --whitespace \
  --pid-recursive \
  --pid"
  
alias ips="/usr/bin/get_iplayer"